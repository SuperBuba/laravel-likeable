{
	"name": "nanosolutions/laravel-likeable",
	"description": "Trait for Laravel Eloquent models to allow easy implementation of a 'like' or 'favorite' or 'remember' feature.",
	"license": "MIT",
	"keywords": ["trait", "laravel", "laravel5", "eloquent", "likeable", "likable", "like", "remember", "follow", "favorite", "favourite"],
	"authors": [
		{
			"name": "Petr Cervenka",
			"email": "petr@nanosolutions.io"
		}
	],
	"require": {
		"php": ">=5.5.0",
		"illuminate/database": ">=5.0",
		"illuminate/support": ">=5.0"
	},
	"require-dev": {
		"orchestra/testbench": "~3.0",
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "~0.9"
	},
	"autoload": {
		"psr-4": {
			"Nano\\Likeable\\": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	}
}
